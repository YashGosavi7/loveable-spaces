
# Environment variables to force offline mode and prevent dev-server lookups
NODE_ENV=production
SKIP_DEV_SERVER=true
VITE_DISABLE_DEV_SERVER=true
NODE_PATH=./node_modules
DEV_SERVER_PATH=./dev-server
NPM_CONFIG_OFFLINE=true
NPM_CONFIG_REGISTRY=https://registry.npmjs.org/
NPM_CONFIG_NO_PROXY=/dev-server,dev-server
